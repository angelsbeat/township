end

function loby()
gg.setVisible(false)
lobyAlert = gg.alert(os.date([[
Edited By Winner Win
Today : %d/%m/%Y
Time : %H:%M ]]), "[ᴍᴇɴᴜ]", "[ᴇxɪᴛ]")
if lobyAlert == 2 then
Exit()
end
end

HOME = 1
SPEEDFUNCTIONNI = 1
loby()

function Main()
mainMenu = gg.choice({
'?⇲ Player ✓ \n ╰〖 MENU〗',
'?⇲ Status ✓ \n ╰〖 MENU〗',
'?⇲ Mob✓ \n ╰〖 MENU〗',
'?⇲ coming soon✓ \n ╰〖 MENU〗',
'〖 EXIT〗'

}, nil, (os.date([[
Created By Winner Win
Today : %d/%m/%Y
Time : %H:%M ]])) )

if mainMenu == 1 then
player() -- ini nge inject ke
end
if mainMenu == 2 then
status()
end
if mainMenu == 3 then
mob()
end
if mainMenu == 4 then
etc()
end
if mainMenu == 5 then
Exit()
end
GLWW = -1
end

function player() --sini, trus ada urutan lagi
playerMenu = gg.choice({
'?⇲ GodMod ✓ \n ╰〖 ON/OFF〗',
'?⇲ Storm AOE\n ╰〖 ON/OFF〗',
'?⇲ MATK ✓ \n ╰〖 ON/OFF〗',
'?⇲ CSPD ✓ \n ╰〖 ON/OFF〗',
'〖 Back〗'

}, nil, (os.date([[
Created by yhuda elsadai
Today : %d/%m/%Y
Time : %H:%M ]])) )

if playerMenu == 1 then -- ini kode fungsi nya
if godmode == on then
godmode1(on)
godmode = off
else
godmode2(off)
godmode = on
end
end
if playerMenu == 2 then
if strmrnge == on then
strmrnge1(on)
strmrnge = off
else
strmrnge2(off)
strmrnge = on
end
end
if playerMenu == 3 then
if MATK== on then
MATK1(on)
MATK = off
else
MATK2(off)
MATK = on
end
end
if playerMenu == 4 then
if cspd == on then
cspd1(on)
cspd = off
else
cspd2(off)
cspd = on
end
end
if playerMenu == 5 then
Main()
end
GLWW = -1
end

