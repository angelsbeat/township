function strmrnge1(on)
local vTable = {}
-- StormRange
vTable[1] = {}
vTable[1].address = baseAddress[1].start + 0x80AA90
vTable[1].flags = gg.TYPE_FLOAT
vTable[1].value = '0E2AB3EEr'

gg.setValues(vTable)
gg.toast('StormArea On')
end

function strmrnge2(off)
local vTable = {}
-- StormRange
vTable[1] = {}
vTable[1].address = baseAddress[1].start + 0x80AA90
vTable[1].flags = gg.TYPE_FLOAT
vTable[1].value = '0C2AB7EEr'

gg.setValues(vTable)
gg.toast('StormArea Off')
end

